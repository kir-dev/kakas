{
  "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
  },
  "rules": {
      "semi": ["error", "always" ],
      "no-trailing-spaces": "error",
      "no-unused-expressions": "error",
      "no-unused-vars":"error",
      "quotes": [ "error", "single" ],
      "spaced-comment": "warn",
      "space-infix-ops": "warn",
      "space-before-blocks": "warn",
      "no-console": "warn",
      "no-var": "error",
      "prefer-const": "warn",
      "prefer-arrow-callback": "warn",
      "eqeqeq": "error",
      "no-empty-function": "error",
      "no-empty": "error",
      "no-shadow": "warn",
      "no-use-before-define": "error"
  }
}